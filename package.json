{
  "license": "MIT",
  "version": "1.0.0",
  "main": "dist/index.js",
  "name": "grpc-typescript-bun",
  "author": "Sandip Patel<sandip254@gmail.com>",
  "description": "Production-ready gRPC service with TypeScript and Bun",
  "scripts": {
    "test": "bun test",
    "start": "bun dist/index.js",
    "lint:types": "tsc --noEmit",
    "dev": "bun --watch src/index.ts",
    "client": "bun src/client/test.ts",
    "build": "bun run generate && tsc",
    "lint:style": "eslint src --ext .ts",
    "clean": "rm -rf dist node_modules bun.lockb",
    "lint": "bun run lint:types && bun run lint:style",
    "docker:build": "docker build -t grpc-typescript .",
    "docker:run": "docker run -p 50051:50051 grpc-typescript",
    "generate": "grpc_tools_node_protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=./src/generated --js_out=import_style=commonjs,binary:./src/generated --grpc_out=grpc_js:./src/generated -I ./proto ./proto/*.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.15",
    "google-protobuf": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "^20.19.9",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.3",
    "grpc-tools": "^1.13.0",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "keywords": [
    "bun",
    "grpc",
    "typescript",
    "microservices"
  ],
  "bun": {
    "workspaces": [],
    "module": "commonjs"
  }
}